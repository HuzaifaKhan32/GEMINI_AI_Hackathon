{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "QDRANT_HOST": "@qdrant_host",
    "QDRANT_API_KEY": "@qdrant_api_key",
    "GOOGLE_API_KEY": "@google_api_key"
  }
}